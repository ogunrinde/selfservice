<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>MultiChase Dashboard</title>

  <!-- Custom fonts for this template-->
  <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template-->
  <link href="css/sb-admin-2.min.css" rel="stylesheet">
  <link rel="stylesheet" type="text/css" href="css/dashboard.css">

</head>

<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">
    <% include sidebar.ejs %>

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <!-- Topbar -->
        <% include top_nav.ejs %>
        <!-- End of Topbar -->

        <div class="container-fluid">
          <div class="flex_container">
            <div class="card bg-success">
              <div class="eachcard">
                 <div class="icon"><i class="fa fa-users"></i></div>
                 <div class="title" style="text-align: center;">
                   <span class="number">22</span><br/>
                   <span>Employee</span>
                 </div>
              </div>
              <div style="" class="text-success">
                <p>View More</p>
              </div>
            </div>
            <div class="card bg-primary">
              <div class="eachcard">
                 <div class="icon"><i class="fa fa-book"></i></div>
                 <div class="title" style="text-align: center;">
                   <span class="number">22</span><br/>
                   <span>Appraisal</span>
                 </div>
              </div>
              <div style="" class="text-primary">
                <p>View More</p>
              </div>
            </div>
            <div class="card bg-dark">
              <div class="eachcard">
                 <div class="icon"><i class="fa fa-bars"></i></div>
                 <div class="title" style="text-align: center;">
                   <span class="number">22</span><br/>
                   <span>Report</span>
                 </div>
              </div>
              <div class="text-dark">
                <p class="" style="color: #fff;"></p>
              </div>
            </div>
            <div class="card bg-danger">
              <div class="eachcard">
                 <div class="icon"><i class="fa fa-cogs"></i></div>
                 <div class="title" style="text-align: center;">
                   <span class="number">22</span><br/>
                   <span>Settings</span>
                 </div>
              </div>
              <div style="" class="text-danger">
                <p>View More</p>
              </div>
            </div>
          </div>
          <!-- Area Chart -->
          <div class="card border-0 shadow mb-4">
            <div class="row" style="margin-top: 10px;">
                <div class="col-lg-8">
                  <div class="card mb-3">
                    <div class="card-header">
                      <i class="fas fa-chart-bar"></i>
                      Performance</div>
                    <div class="card-body">
                      <canvas id="myBarChart" width="100%" height="50"></canvas>
                    </div>
                    <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
                  </div>
                </div>
                <div class="col-lg-4">
                  <div class="card mb-3">
                    <div class="card-header">
                      <i class="fas fa-chart-pie"></i>
                      Analysis</div>
                    <div class="card-body">
                      <canvas id="myPieChart" width="100%" height="100"></canvas>
                    </div>
                    <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
                  </div>
                </div>
              </div>
          </div>

        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      <footer class="sticky-footer bg-white">
        <div class="container my-auto">
          <div class="copyright text-center my-auto">
            <span></span>
          </div>
        </div>
      </footer>
      <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>


  <!-- Bootstrap core JavaScript-->
  <script src="vendor/jquery/jquery.min.js"></script>
  <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="js/sb-admin-2.min.js"></script>

  <!-- Page level plugins -->
  <script src="vendor/chart.js/Chart.min.js"></script>

  <!-- Page level custom scripts -->
  <script src="js/js/demo/chart-area-demo.js"></script>
      <!-- Custom scripts for all pages-->
      <script src="js/js/sb-admin-2.min.js"></script>

      <!-- Demo scripts for this page-->
      <script src="js/js/demo/chart-area-demo.js"></script>
      <script src="js/js/demo/chart-bar-demo.js"></script>
      <script src="js/js/demo/chart-pie-demo.js"></script>

</body>

</html>
